\n    <div style=\"width: 400px;margin: 150px auto\">\r\n     <div style=\"color: #cccccc;font-size: 30px;text-align: center;padding:30px 0\">欢迎登陆艺术品平台</div>\r\n<!--      <el-form ref=\"form\" :model=\"form\" size=\"normal\" :rules=\"rules\">-->\r\n      <el-form ref=\"form\" :model=\"form\" size=\"normal\" :rules=\"rules\">\r\n<!--        输入框-->\r\n        <el-form-item prop =\"userName\" >\r\n          <el-input :prefix-icon=\"User\" v-model=\"form.userName\" clearable ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"userPassword\">\r\n          <el-input :prefix-icon=\"Lock\" v-model=\"form.userPassword\" clearable show-password ></el-input>\r\n        </el-form-item>\r\n<!--        按钮-->\r\n      <div style=\"display: flex\">\r\n        <el-form-item style=\"margin-right:10%;width: 120%\">\r\n          <el-button style=\"width: 160%;\" type=\"primary\" @click=\"login\">用 户 登 录</el-button>\r\n        </el-form-item>\r\n        <el-form-item style=\"width: 120%\">\r\n          <el-button style=\"width: 160%;\" type=\"primary\" @click=\"adminlogin\">管 理 者 登 录</el-button>\r\n        </el-form-item>\r\n      </div>\r\n\r\n        <el-form-item >\r\n          <el-button style=\"width: 100%;\" type=\"primary\" @click=\"$router.push('/register')\">新用户注册</el-button>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n  import request from \"@/utils/request\";\r\n  export default {\r\n    name:\"Login\",\r\n    data(){\r\n      let letterRule = (rule, value, callback) => {\r\n            let reg = /^[a-zA-Z0-9_-]{4,18}$/\r\n            if (value === '') {\r\n              callback(new Error('输入内容不能为空'));\r\n            }else if(!reg.test(value)) {\r\n              // callback(new Error('请输入字母、数字、下划线'));\r\n            }else {\r\n              callback();\r\n            }\r\n      };\r\n      return{\r\n        myform:{},\r\n        form:{},\r\n        rules:{\r\n          userName:[\r\n            { required: true, message: '请输入用户名', trigger: 'blur' },\r\n            { min: 1, max: 18, message: '长度在 1 到 18 个字符', trigger: 'blur' },\r\n            { validator: letterRule, trigger: 'blur' }\r\n          ],\r\n\r\n          userPassword:[\r\n            { required: true, message: '请输入密码', trigger: 'blur' },\r\n            { min: 1, max: 18, message: '长度在 1 到 18 个字符', trigger: 'blur' },\r\n            { validator: letterRule, trigger: 'blur' }\r\n          ]\r\n        }\r\n      }\r\n    },\r\n    methods:{\r\n      //用户登录\r\n      login(){\r\n            console.log('login')\r\n            console.log(this.form)\r\n            request.post(\"/dataj/user/login\",this.form).then(res=>{\r\n              if(res.code==='0')\r\n              {\r\n                this.$message({\r\n                  type:\"message\",\r\n                  message:\"登录成功\"\r\n                })\r\n                this.$router.push(\"/\")\r\n              }\r\n              else{\r\n                this.$message({\r\n                  type:\"error\",\r\n                  message:res.msg\r\n                })\r\n              }\r\n            })\r\n          },\r\n      //管理者登陆\r\n      adminlogin(){\r\n        console.log('adminlogin')\r\n        this.myform.adminAccount=this.form.userName\r\n        this.myform.adminPassword=this.form.userPassword\r\n        console.log(this.myform)\r\n        request.post(\"/dataj/admin/login\",this.myform).then(res=>{\r\n          if(res.code==='0')\r\n          {\r\n            this.$message({\r\n              type:\"message\",\r\n              message:\"登录成功\"\r\n            })\r\n            this.$router.push(\"/admin\")\r\n          }\r\n          else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }\r\n      }\r\n  }\r\n\r\n</script>\r\n\r\n<script setup>\r\nimport {Lock,User } from '@element-plus/icons-vue'\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}