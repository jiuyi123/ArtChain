"Aside","name","data","url","form","search","currentPage","pageSize","total","dialogVisible","tableData","created","load","methods","state","row","column","nftLoc","console","log","get","params","pageNum","then","res","records","add","save","userId","put","code","$message","type","message","msg","post","handleDelete","delete","handleEdit","JSON","parse","stringify","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["D:\\VUESPRINT\\pvue\\springboot-vue-demo\\vue\\src\\views\\Admin\\StmInfoMagt\\ArtsCheck.vue"],"sourcesContent":["<template>\r\n  <div style=\"display: flex;\">\r\n    <div style=\"padding: 5px\">\r\n      <div style=\"text-align: center;font-size: 50px;border-width: thick;font-family: 华文行楷;margin: 10px\">\r\n        <p>艺   术   品   管   理</p>\r\n      </div>\r\n      <!--搜索+功能-->\r\n      <el-card>\r\n        <!--    搜索区域-->\r\n        <div style=\"margin: 0px\">\r\n          <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 80%\" clearable/>\r\n          <el-button type=\"primary\" style=\"margin: 2px;margin-left: 20px\" @click=\"load\">查询</el-button>\r\n          <el-button type=\"primary\" @click=\"add\">新增</el-button>\r\n          <!--    功能区域-->\r\n<!--          <div style=\"margin: 10px 0\">-->\r\n<!--            <el-button type=\"primary\" @click=\"add\">新增</el-button>-->\r\n<!--            <el-button type=\"primary\" @click=\"daorv\">导入</el-button>-->\r\n<!--            <el-button type=\"primary \" @click=\"getvnt\">导出</el-button>-->\r\n<!--          </div>-->\r\n        </div>\r\n      </el-card>\r\n      <!--表格+分页-->\r\n      <el-card style=\"margin-top: 5px\">\r\n        <!--    表格区域-->\r\n        <el-scrollbar height=\"480px\" style=\"border: #3E3E3E\">\r\n          <el-table :data=\"tableData\"  stripe border style=\"width:100%;\" >\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftId\"\r\n              label=\"ID\"\r\n              width=\"80%\"\r\n              sortable/>\r\n          <el-table-column   align=\"center\" label=\"艺术品\" width=\"180%\">\r\n<!--            prop=\"nftLoc\"-->\r\n            <!--            <el-avatar  shape=\"square\" :size=\"30\" :src=\"url\"/>-->\r\n\r\n            <template #default=\"scope\">\r\n              <el-image\r\n                style=\"width: 120px;height: 100px\"\r\n                :src=\"scope.row.nftLoc\"\r\n                :preview-teleported=\"true\"\r\n                :preview-src-list=\"[scope.row.nftLoc]\">\r\n              </el-image>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftName\"\r\n              label=\"艺术品名称\"\r\n              width=\"200%\"/>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftPrice\"\r\n              label=\"价值\"\r\n              width=\"80%\"/>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftWriterId\"\r\n              label=\"作者ID\"\r\n              width=\"80%\"/>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftOwnerId\"\r\n              label=\"拥有者ID\"\r\n              width=\"90%\"/>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftSta\"\r\n              label=\"上架/下架\"\r\n              width=\"100%\"/>\r\n\r\n          <el-table-column\r\n              align=\"center\"\r\n              prop=\"nftHash\"\r\n              label=\"艺术品哈希值\"\r\n              width=\"250%\"/>\r\n          <!--操作区-->\r\n          <el-table-column   align=\"center\" fixed=\"right\" label=\"Operations\" width=\"200%\">\r\n            <template #default=\"scope\">\r\n\r\n              <el-button type=\"default\" size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n              <el-popconfirm title=\"Are you sure to delete this?\"  @confirm=\"handleDelete(scope.row.userId)\">\r\n                <template #reference>\r\n                  <el-button type=\"danger\" size=\"mini\">删除</el-button>\r\n                </template>\r\n              </el-popconfirm>\r\n\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        </el-scrollbar>\r\n        <!--    分页-->\r\n        <el-pagination\r\n            style=\"text-align: center;margin-left: 30%;margin-top: 5px\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            v-model:currentPage=\"currentPage\"\r\n            :page-size=\"pageSize\"\r\n            :total=\"total\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :small=\"small\"\r\n            :disabled=\"disabled\"\r\n            :background=\"background\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n        >\r\n        </el-pagination>\r\n      </el-card>\r\n      <!--      对话框-->\r\n      <div style=\"margin: 10px 0\">\r\n        <el-dialog v-model=\"dialogVisible\" title=\"Tips\" width=\"30%\">\r\n          <el-form :model=\"form\" label-width=\"120px\">\r\n            <el-form-item label=\"用户名\">\r\n              <el-input v-model=\"form.userName\" style=\"width: 80%\"></el-input>\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"密码\">\r\n              <el-input v-model=\"form.userPassword\" style=\"width: 80%\"></el-input>\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"银行卡号\">\r\n              <el-input v-model=\"form.userCard\" style=\"width: 80%\"></el-input>\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"用户地址\">\r\n              <el-input v-model=\"form.userAddr\" style=\"width: 80%\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n\r\n          <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"save\">保存</el-button>\r\n          </span>\r\n          </template>\r\n\r\n        </el-dialog>\r\n\r\n      </div>\r\n      <!--    <el-button @click=\"go1\">点我跳转</el-button>-->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Aside from \"@/components/Aside\";\r\nexport default {\r\n  name: 'Home',\r\n  data(){\r\n    return {\r\n      url:\"https://fuss10.elemecdn.com/e/5d/4a731a90594a4af544c0c25941171jpeg.jpeg\",\r\n      //vnt,\r\n      form: {},\r\n      search:'',\r\n      currentPage:1,\r\n      pageSize:10,\r\n      total:0,\r\n      dialogVisible:false,\r\n      tableData: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    state(row, column) {\r\n      this.url=row.nftLoc\r\n      console.log(row.nftLoc)\r\n    },\r\n\r\n    //加载页面\r\n    load(){//从数据库加载数据渲染到前端页面\r\n      request.get(\"/dataj/nft\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n    //新增/更新数据\r\n    add(){//新增数据弹窗\r\n      this.dialogVisible=true;\r\n      this.form={};//清空表单\r\n    },\r\n    save(){//将弹窗数据post到数据库\r\n      console.log(this.form.userId);\r\n      if(this.form.userId){\r\n        //更新\r\n        request.put(\"/dataj/user/update\",this.form).then(res=>{\r\n          console.log(res);\r\n          if(res.code==='0')\r\n          {\r\n            this.$message({\r\n              type:\"message\",\r\n              message:\"更新成功\"\r\n            })\r\n          }\r\n          else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }\r\n      else{\r\n        request.post(\"/dataj/user/insert\",this.form).then(\r\n            res=>{\r\n              //执行完毕后结果回到res这\r\n              console.log(res);\r\n              //弹窗提示正确\r\n              if(res.code==='0')\r\n              {\r\n                this.$message({\r\n                  type:\"message\",\r\n                  message:\"新增成功\"\r\n                })\r\n              }\r\n              else{\r\n                this.$message({\r\n                  type:\"error\",\r\n                  message:res.msg\r\n                })\r\n              }\r\n            })\r\n      }\r\n      this.dialogVisible=false;\r\n      this.load();\r\n    },\r\n    //删除数据\r\n    handleDelete(userId){\r\n      console.log(userId)\r\n      request.delete(\"/dataj/user/\"+userId).then(res=>{\r\n        if(res.code==='0')\r\n        {\r\n          this.$message({\r\n            type:\"message\",\r\n            message:\"更新成功\"\r\n          })\r\n        }\r\n        else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n\r\n      this.load()\r\n    },\r\n    //编辑更新数据\r\n    handleEdit(row){\r\n      //cancel不会改变\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible=true\r\n    },\r\n    //页面容量调整\r\n    handleSizeChange(pageSize){//改变当前每页容纳个数\r\n      this.pageSize=pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){ //改变当前页码触发\r\n      this.currentPage=pageNum\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n"]}]}